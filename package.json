{
	"name": "tetsuo",
	"version": "0.0.1",
	"main": "./lib/index.js",
	"license": "MIT",
	"prettier": {
		"da": true
	},
	"lint-staged": {
		"verbose": true,
		"*.js": [
			"prettier --write",
			"git add"
		]
	},
	"babel": {
		"presets": [
			"es2015",
			"flow"
		],
		"plugins": [
			"add-module-exports",
			"array-includes"
		],
		"env": {
			"build": {
				"plugins": [
					"transform-regenerator"
				]
			},
			"test": {
				"presets": [
					"power-assert"
				],
				"plugins": [
					"istanbul"
				]
			}
		}
	},
	"nyc": {
		"require": [
			"babel-register"
		],
		"sourceMap": false,
		"instrument": false
	},
	"scripts": {
		"flow": "flow",
		"nycha": "cross-env NODE_ENV=test nyc mocha",
		"test": "run-s flow nycha",
		"coverage": "nyc report --reporter=text-lcov | coveralls",
		"build": "cross-env NODE_ENV=build babel ./src -d ./lib",
		"precommit": "lint-staged",
		"typed": "flow-typed"
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-plugin-add-module-exports": "^0.2.1",
		"babel-plugin-array-includes": "^2.0.3",
		"babel-plugin-istanbul": "^4.1.4",
		"babel-plugin-transform-regenerator": "^6.26.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-flow": "^6.23.0",
		"babel-preset-power-assert": "^1.0.0",
		"babel-register": "^6.26.0",
		"coveralls": "^2.13.1",
		"cross-env": "^5.0.5",
		"flow-bin": "^0.54.0",
		"flow-typed": "^2.1.5",
		"husky": "^0.14.3",
		"lint-staged": "^4.0.4",
		"mocha": "^3.5.0",
		"npm-run-all": "^4.1.1",
		"nyc": "^11.1.0",
		"power-assert": "^1.4.4",
		"prettier": "^1.6.1"
	},
	"dependencies": {
		"regenerator-runtime": "^0.11.0"
	}
}
